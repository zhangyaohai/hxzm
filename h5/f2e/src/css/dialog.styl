.ubike-mask 
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);

.ubike-dialog
  width: 35%
  position: fixed;
  z-index: 5000;
  width: 80%;
  max-width: 300px;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #FFFFFF;
  text-align: center;
  border-radius: 3px;
  overflow: hidden;

.ubike-dialog_hd
  padding: 1.3em 1.6em 0.5em;

.ubike-dialog_bd
  padding: 0 1.6em 0.8em;
  min-height: 40px;
  font-size: 15px;
  line-height: 1.3;
  word-wrap: break-word;
  word-break: break-all;
  color: #999999;

.ubike-dialog_ft
  position: relative;
  line-height: 48px;
  font-size: 18px;
  display: box;
  display: flex;
  height: 48px

.ubike-dialog_ft:after
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #D5D5D6;
  color: #D5D5D6;
  transform-origin: 0 0;
  origin: 0 0;
  transform: scaleY(0.5);

.ubike-dialog_hd_title
  font-weight: 400;
  font-size: 18px;

.ubike-dialog-confirm
  display: block;
  box-flex: 1;
  flex: 1;
  color: #353535;
  background-color: #fff
  font-size: 16px
  border-left: 1px solid #D5D5D6;
  font-weight: 600
  text-decoration: none;
  tap-highlight-color: rgba(0, 0, 0, 0);
  position: relative;



